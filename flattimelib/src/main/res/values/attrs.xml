        <attr name="minIndicatorColor" format="color" />
        <attr name="hourIndicatorColor" format="color" />
        <attr name="secIndicatorColor" format="color" />
        <attr name="bigMarkColor" format="color" />
        <attr name="smallMarkColor" format="color" />
        <attr name="backgroundCircleColor" format="color" />
        <attr name="time" format="string" />
